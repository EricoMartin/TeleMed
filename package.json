{
  "name": "server",
  "version": "1.0.0",
  "description": "Backend for TeleMed Health App",
  "main": "app.js",
  "scripts": {
    "start": "node ./server/app.js ",
    "start:dev": "nodemon node ./server/app.js",
    "build": "npm run clean && mkdir dist && babel server -d dist --copy-files",
    "clean": "rm -rf dist",
    "lint": "eslint ./server",
    "lint-fix": "npm run lint  -- --fix ./server/* ",
    "test": "jest --no-cache --detectOpenHandles",
    "test:watch": "jest --watch",
    "heroku-prebuild": "rm -rf node_modules"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@sendgrid/mail": "^6.5.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.18.1",
    "cors": "^2.8.5",
    "coveralls": "^3.0.9",
    "datauri": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "jshint": "^2.11.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.9",
    "multer": "^1.4.2",
    "multer-storage-cloudinary": "^2.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  }
}
