{
  "name": "server",
  "version": "1.0.0",
  "description": "Backend for TeleMed Health App",
  "main": "app.js",
  "scripts": {
    "start": "babel-node ./server/app.js ",
    "start:dev": "nodemon babel-node ./server/app.js",
    "build": "npm run clean && mkdir dist && babel server -d dist --copy-files",
    "clean": "rm -rf dist",
